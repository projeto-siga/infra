<% @data_sources.each_pair do |name, ds| %>
  data-source add \
  --name=<%= name %> \
  <% ds.each do |param, value| %>
    <% if param != "security-domain-username" && param != "security-domain-password" %>
      --<%= param %>=<%= value %> \
    <% end %>
  <% end %>
  --use-java-context=true

<% end %>
